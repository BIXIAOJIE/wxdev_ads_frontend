<!--pages/login/login.wxml-->

<form bindsubmit="formSubmit" bindreset="formReset">
<button  type="primary" bindtap="jumpToIndex">返回首页</button>

  <view class="content">
    <view class="account">
      <view class="title">姓名</view>
      <view class="num">
        <input name="name" placeholder-style="color:#999999;" />
      </view>
    </view>
    <view class="hr"></view>
  </view>

    <view class="content">
    <view class="account">
      <view class="title">孕周</view>
      <view class="num">
        <input name="gestation" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
  <view class="hr"></view>

    <view class="content">
    <view class="account">
      <view class="title">年龄</view>
      <view class="num">
        <input name="age" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
  <view class="hr"></view>

    <view class="content">
    <view class="account">
      <view class="title">电话</view>
      <view class="num">
        <input name="phone" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
<view class="hr"></view>
    <view class="content">
    <view class="account">
      <view class="title">地址</view>
      <view class="num">
        <input name="address" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
<view class="hr"></view>
  <view class="content">
    <view class="account">
      <view class="title">渠道</view>
      <view class="num">
        <input name="channel" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
<view class="hr"></view>
  <view class="content">
    <view class="account">
      <view class="title">经理</view>
      <view class="num">
        <input name="assistant_manager" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
<view class="hr"></view>

  <view class="content">
    <view class="account">
      <view class="title">备注</view>
      <view class="num">
        <input name="note" placeholder-style="color:#999999;" />
      </view>
    </view>
  </view>
  <view class="btn-area">
    <button formType="submit">Submit</button>
    <button formType="reset">Reset</button>
  </view>
</form>